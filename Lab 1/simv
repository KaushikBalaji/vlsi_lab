#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x627d8706a430 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x627d8706a5c0 .scope module, "tb_fibo" "tb_fibo" 3 1;
 .timescale 0 0;
v0x627d8707e790_0 .var "clk", 0 0;
v0x627d8707e860_0 .net "fib_out", 31 0, v0x627d8707e560_0;  1 drivers
v0x627d8707e930_0 .var "reset", 0 0;
S_0x627d8702f490 .scope module, "fib_1" "fibo_nonmodular" 3 6, 4 2 0, S_0x627d8706a5c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 32 "reg1";
P_0x627d8702f620 .param/l "N" 0 4 2, +C4<00000000000000000000000000100000>;
v0x627d8702f710_0 .net "clk", 0 0, v0x627d8707e790_0;  1 drivers
v0x627d8707e480_0 .var "reg0", 31 0;
v0x627d8707e560_0 .var "reg1", 31 0;
v0x627d8707e650_0 .net "reset", 0 0, v0x627d8707e930_0;  1 drivers
E_0x627d8702fa30 .event posedge, v0x627d8702f710_0;
    .scope S_0x627d8702f490;
T_0 ;
    %wait E_0x627d8702fa30;
    %load/vec4 v0x627d8707e650_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x627d8707e560_0, 0;
    %pushi/vec4 1, 0, 32;
    %assign/vec4 v0x627d8707e480_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x627d8707e480_0;
    %load/vec4 v0x627d8707e560_0;
    %add;
    %load/vec4 v0x627d8707e480_0;
    %concat/vec4; draw_concat_vec4
    %split/vec4 32;
    %assign/vec4 v0x627d8707e560_0, 0;
    %assign/vec4 v0x627d8707e480_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x627d8706a5c0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x627d8707e790_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x627d8706a5c0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x627d8707e790_0;
    %inv;
    %store/vec4 v0x627d8707e790_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x627d8706a5c0;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x627d8707e930_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x627d8707e930_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x627d8707e930_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x627d8707e930_0, 0, 1;
    %delay 60, 0;
    %vpi_call/w 3 14 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x627d8706a5c0;
T_4 ;
    %vpi_call/w 3 17 "$display", "Time | reset | fib_out" {0 0 0};
    %vpi_call/w 3 18 "$monitor", "%0t |   %b   | %0d", $time, v0x627d8707e930_0, v0x627d8707e860_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "testbench.v";
    "fibo.v";
